{
  "printWidth": 80,
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 4,
  "semi": true,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "trailingComma": "es5",
  "arrowParens": "always",
  "proseWrap": "preserve",
  "quoteProps": "preserve",
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "auto"
}
